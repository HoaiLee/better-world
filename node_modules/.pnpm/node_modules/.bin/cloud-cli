#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hoaile/Documents/Code/BetterWorld/better-world/node_modules/.pnpm/@strapi+cloud-cli@5.18.1/node_modules/@strapi/cloud-cli/bin/node_modules:/Users/hoaile/Documents/Code/BetterWorld/better-world/node_modules/.pnpm/@strapi+cloud-cli@5.18.1/node_modules/@strapi/cloud-cli/node_modules:/Users/hoaile/Documents/Code/BetterWorld/better-world/node_modules/.pnpm/@strapi+cloud-cli@5.18.1/node_modules/@strapi/node_modules:/Users/hoaile/Documents/Code/BetterWorld/better-world/node_modules/.pnpm/@strapi+cloud-cli@5.18.1/node_modules:/Users/hoaile/Documents/Code/BetterWorld/better-world/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/hoaile/Documents/Code/BetterWorld/better-world/node_modules/.pnpm/@strapi+cloud-cli@5.18.1/node_modules/@strapi/cloud-cli/bin/node_modules:/Users/hoaile/Documents/Code/BetterWorld/better-world/node_modules/.pnpm/@strapi+cloud-cli@5.18.1/node_modules/@strapi/cloud-cli/node_modules:/Users/hoaile/Documents/Code/BetterWorld/better-world/node_modules/.pnpm/@strapi+cloud-cli@5.18.1/node_modules/@strapi/node_modules:/Users/hoaile/Documents/Code/BetterWorld/better-world/node_modules/.pnpm/@strapi+cloud-cli@5.18.1/node_modules:/Users/hoaile/Documents/Code/BetterWorld/better-world/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@strapi/cloud-cli/bin/index.js" "$@"
else
  exec node  "$basedir/../@strapi/cloud-cli/bin/index.js" "$@"
fi
