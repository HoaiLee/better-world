#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hoaile/Documents/Code/BetterWorld/better-world/node_modules/.pnpm/@strapi+strapi@5.18.1_@babel+runtime@7.27.6_@codemirror+autocomplete@6.18.6_@codemirror_4b826d624ec984481b0b507e471a4489/node_modules/@strapi/strapi/bin/node_modules:/Users/hoaile/Documents/Code/BetterWorld/better-world/node_modules/.pnpm/@strapi+strapi@5.18.1_@babel+runtime@7.27.6_@codemirror+autocomplete@6.18.6_@codemirror_4b826d624ec984481b0b507e471a4489/node_modules/@strapi/strapi/node_modules:/Users/hoaile/Documents/Code/BetterWorld/better-world/node_modules/.pnpm/@strapi+strapi@5.18.1_@babel+runtime@7.27.6_@codemirror+autocomplete@6.18.6_@codemirror_4b826d624ec984481b0b507e471a4489/node_modules/@strapi/node_modules:/Users/hoaile/Documents/Code/BetterWorld/better-world/node_modules/.pnpm/@strapi+strapi@5.18.1_@babel+runtime@7.27.6_@codemirror+autocomplete@6.18.6_@codemirror_4b826d624ec984481b0b507e471a4489/node_modules:/Users/hoaile/Documents/Code/BetterWorld/better-world/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/hoaile/Documents/Code/BetterWorld/better-world/node_modules/.pnpm/@strapi+strapi@5.18.1_@babel+runtime@7.27.6_@codemirror+autocomplete@6.18.6_@codemirror_4b826d624ec984481b0b507e471a4489/node_modules/@strapi/strapi/bin/node_modules:/Users/hoaile/Documents/Code/BetterWorld/better-world/node_modules/.pnpm/@strapi+strapi@5.18.1_@babel+runtime@7.27.6_@codemirror+autocomplete@6.18.6_@codemirror_4b826d624ec984481b0b507e471a4489/node_modules/@strapi/strapi/node_modules:/Users/hoaile/Documents/Code/BetterWorld/better-world/node_modules/.pnpm/@strapi+strapi@5.18.1_@babel+runtime@7.27.6_@codemirror+autocomplete@6.18.6_@codemirror_4b826d624ec984481b0b507e471a4489/node_modules/@strapi/node_modules:/Users/hoaile/Documents/Code/BetterWorld/better-world/node_modules/.pnpm/@strapi+strapi@5.18.1_@babel+runtime@7.27.6_@codemirror+autocomplete@6.18.6_@codemirror_4b826d624ec984481b0b507e471a4489/node_modules:/Users/hoaile/Documents/Code/BetterWorld/better-world/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/strapi.js" "$@"
else
  exec node  "$basedir/../../bin/strapi.js" "$@"
fi
