#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hoaile/Documents/Code/BetterWorld/better-world/node_modules/.pnpm/astro@5.12.0_@types+node@20.19.9_jiti@2.4.2_lightningcss@1.30.1_rollup@4.45.1_sass@1.89_626cb9dad0a4445124a488c3f433be05/node_modules/astro/node_modules:/Users/hoaile/Documents/Code/BetterWorld/better-world/node_modules/.pnpm/astro@5.12.0_@types+node@20.19.9_jiti@2.4.2_lightningcss@1.30.1_rollup@4.45.1_sass@1.89_626cb9dad0a4445124a488c3f433be05/node_modules:/Users/hoaile/Documents/Code/BetterWorld/better-world/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/hoaile/Documents/Code/BetterWorld/better-world/node_modules/.pnpm/astro@5.12.0_@types+node@20.19.9_jiti@2.4.2_lightningcss@1.30.1_rollup@4.45.1_sass@1.89_626cb9dad0a4445124a488c3f433be05/node_modules/astro/node_modules:/Users/hoaile/Documents/Code/BetterWorld/better-world/node_modules/.pnpm/astro@5.12.0_@types+node@20.19.9_jiti@2.4.2_lightningcss@1.30.1_rollup@4.45.1_sass@1.89_626cb9dad0a4445124a488c3f433be05/node_modules:/Users/hoaile/Documents/Code/BetterWorld/better-world/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../astro.js" "$@"
else
  exec node  "$basedir/../../astro.js" "$@"
fi
